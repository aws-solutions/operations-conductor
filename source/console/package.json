{
    "name": "operations-conductor-console",
    "description": "The Operations Conductor Web console",
    "version": "1.0.4",
    "private": true,
    "dependencies": {
        "@aws-amplify/api": "^4.0.0",
        "@aws-amplify/auth": "^4.0.0",
        "@aws-amplify/core": "^4.0.0",
        "@aws-amplify/ui-react": "^3.0.0",
        "bootstrap": "^3.3.7",
        "react": "^16.11.0",
        "react-bootstrap": "^0.32.4",
        "react-dom": "^16.11.0",
        "react-router-bootstrap": "^0.25.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^5.0.1",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@testing-library/user-event": "^14.4.3",
        "@types/react": "^16.9.9",
        "@types/react-bootstrap": "^0.32.20",
        "@types/react-dom": "^16.9.2",
        "@types/react-router-bootstrap": "^0.24.5",
        "@types/react-router-dom": "^5.1.0",
        "eslint-plugin-jest-dom": "^4.0.3",
        "msw": "^0.49.1",
        "typescript": "~4.4.0"
    },
    "overrides": {
        "nth-check": "^2.0.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "npm ci && react-scripts build",
        "test": "CI=true react-scripts test --coverage",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "testMatch": [
            "**/*.test.ts(x)?"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/react-app-env.d.ts",
            "!src/types/aws-amplify-react.d.ts",
            "!src/index.tsx"
        ]
    }
}
